{
    "name": "tonkeeper-desktop",
    "version": "3.0.0",
    "description": "Your desktop wallet on The Open Network",
    "main": ".webpack/main",
    "scripts": {
        "locales": "ts-node ./task/locales",
        "start": "ELECTRON_IS_DEV=1 electron-forge start",
        "package": "ELECTRON_IS_DEV=0 electron-forge package",
        "make": "ELECTRON_IS_DEV=0 electron-forge make",
        "publish": "ELECTRON_IS_DEV=0 electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "build:win32": "npm run make -- --platform win32",
        "build:linux": "npm run make -- --platform linux",
        "build:darwin": "npm run make -- --platform darwin",
        "build": "npm-run-all -p build:*",
        "build:desktop": "npm-run-all -p build:*"
    },
    "keywords": [],
    "author": {
        "name": "Nikita Kuznetsov",
        "email": "nkuz915@gmail.com"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.2.0",
        "@electron-forge/maker-deb": "^7.2.0",
        "@electron-forge/maker-rpm": "^7.2.0",
        "@electron-forge/maker-squirrel": "^7.2.0",
        "@electron-forge/maker-zip": "^7.2.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
        "@electron-forge/plugin-webpack": "^7.2.0",
        "@electron-forge/shared-types": "^7.2.0",
        "@types/node": "^20.10.3",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "@vercel/webpack-asset-relocator-loader": "1.7.3",
        "css-loader": "^6.0.0",
        "electron": "28.0.0",
        "eslint": "^8.0.1",
        "eslint-plugin-import": "^2.25.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "node-loader": "^2.0.0",
        "npm-run-all": "^4.1.5",
        "style-loader": "^3.0.0",
        "ts-loader": "^9.2.2",
        "ts-node": "^10.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.89.0",
        "yarn-run-all": "^3.1.1"
    },
    "dependencies": {
        "@tanstack/react-query": "4.3.4",
        "@tonkeeper/core": "workspace:0.1.0",
        "@tonkeeper/locales": "workspace:0.1.0",
        "@tonkeeper/uikit": "workspace:0.1.0",
        "buffer": "^6.0.3",
        "copy-to-clipboard": "^3.3.3",
        "electron-is-dev": "^2.0.0",
        "electron-squirrel-startup": "^1.0.0",
        "i18next": "^23.7.7",
        "i18next-http-backend": "^2.4.2",
        "keytar": "^7.9.0",
        "process": "^0.11.10",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "^13.5.0",
        "react-router-dom": "^6.20.1",
        "styled-components": "^6.1.1"
    },
    "packageManager": "yarn@4.0.2"
}
